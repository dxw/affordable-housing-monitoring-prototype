<%= render partial: '/partial/header_internal' %>
<div class="container">
<%= bootstrap_form_for(@development) do |form| %>
  <%= render partial: 'form', locals: { form: form } %>
  <%= form.submit %>
<% end %>

<%= button_to 'Delete Development', development_path(@development), method: :delete, class: 'btn btn-danger mt-2', data: {confirm: 'This will delete all dwellings for this development. Are you sure?'} %>
</div>